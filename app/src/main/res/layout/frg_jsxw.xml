<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#141414"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="@dimen/j10dp">

        <TextView
            style="@style/text_style_13_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="起始" />

        <TextView
            android:id="@+id/mTextView_x1"
            style="@style/text_style_11_white"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/j8dp"
            android:layout_weight="1"
            android:background="@drawable/shape_gray_bk"
            android:drawableRight="@drawable/ic_down_s"
            android:gravity="center_vertical"
            android:hint="请选择"
            android:paddingBottom="@dimen/j3dp"
            android:paddingLeft="@dimen/j4dp"
            android:paddingRight="@dimen/j4dp"
            android:paddingTop="@dimen/j3dp"
            android:singleLine="true"
            android:textColorHint="@color/tyE4" />

        <TextView
            style="@style/text_style_13_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/j8dp"
            android:text="结束" />

        <TextView
            android:id="@+id/mTextView_x2"
            style="@style/text_style_11_white"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/j8dp"
            android:layout_weight="1"
            android:background="@drawable/shape_gray_bk"
            android:drawableRight="@drawable/ic_down_s"
            android:gravity="center_vertical"
            android:hint="请选择"
            android:paddingBottom="@dimen/j3dp"
            android:paddingLeft="@dimen/j4dp"
            android:paddingRight="@dimen/j4dp"
            android:paddingTop="@dimen/j3dp"
            android:singleLine="true"
            android:textColorHint="@color/tyE4" />

        <TextView
            android:id="@+id/mImageView_dl"
            style="@style/text_style_13_white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/j8dp"
            android:background="@drawable/shape_login"
            android:gravity="center"
            android:paddingBottom="@dimen/j3dp"
            android:paddingLeft="@dimen/j8dp"
            android:paddingRight="@dimen/j8dp"
            android:paddingTop="@dimen/j3dp"
            android:text="查询"
            android:textColor="@color/shouye_red" />
    </LinearLayout>

    <com.ab.view.pullview.AbPullListView
        android:id="@+id/mAbPullListView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:cacheColorHint="#00000000"
        android:divider="@null"
        android:fadingEdge="none"
        android:listSelector="#00000000" />
</LinearLayout>

