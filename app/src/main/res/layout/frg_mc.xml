<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#141414"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="@dimen/j60dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/j15dp"
        android:background="@drawable/shape_bg_left"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:paddingBottom="@dimen/j10dp">

        <LinearLayout
            android:id="@+id/mLinearLayout_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="gone">

            <ImageView
                android:id="@+id/mImageView_1"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_fdoorlock_nor" />

            <TextView
                android:id="@+id/mTextView_1"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="左前门锁" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="gone">

            <ImageView
                android:id="@+id/mImageView_2"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_bdoorlock_nor" />

            <TextView
                android:id="@+id/mTextView_2"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="左后门锁" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView_3"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_window_nor" />

            <TextView
                android:id="@+id/mTextView_3"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="左前窗" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView_4"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_window_nor" />

            <TextView
                android:id="@+id/mTextView_4"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="左后窗" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView_5"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_trunk_nor" />

            <TextView
                android:id="@+id/mTextView_5"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="后备箱" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/j15dp"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <TextView
            style="@style/text_style_15_white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="门窗状态"
            android:textColor="@color/A" />

        <TextView
            android:id="@+id/mTextView_state"
            style="@style/text_style_13_white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j10dp"
            android:layout_weight="1"
            android:lineSpacingExtra="@dimen/j8dp"
            android:text="@string/test_mc" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_control_car_top" />

        <TextView
            android:id="@+id/mImageView_cz"
            style="@style/text_style_15_white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/j38dp"
            android:layout_marginBottom="@dimen/j15dp"
            android:layout_marginLeft="@dimen/j10dp"
            android:layout_marginRight="@dimen/j10dp"
            android:layout_marginTop="@dimen/j8dp"
            android:background="@drawable/shape_login"
            android:gravity="center"
            android:padding="@dimen/j6dp"
            android:text="执行操作"
            android:textColor="@color/shouye_red" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="@dimen/j60dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/j15dp"
        android:background="@drawable/shape_bg_right"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:paddingBottom="@dimen/j10dp">

        <LinearLayout
            android:id="@+id/mLinearLayout_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="gone">

            <ImageView
                android:id="@+id/mImageView_6"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_fdoorlock_nor" />

            <TextView
                android:id="@+id/mTextView_6"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="右前门锁" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="gone">

            <ImageView
                android:id="@+id/mImageView_7"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_bdoorlock_nor" />

            <TextView
                android:id="@+id/mTextView_7"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="右后门锁" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView_8"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_window_nor" />

            <TextView
                android:id="@+id/mTextView_8"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="右前窗" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView_9"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_window_nor" />

            <TextView
                android:id="@+id/mTextView_9"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="右后窗" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView_10"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_skylight_nor" />

            <TextView
                android:id="@+id/mTextView_10"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="天窗" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

