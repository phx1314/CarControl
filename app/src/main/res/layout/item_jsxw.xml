<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#141414"
    android:orientation="horizontal">

    <RelativeLayout
        android:layout_width="@dimen/j30dp"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/mImageView_l1"
            android:layout_width="1dip"
            android:layout_height="@dimen/j15dp"
            android:layout_centerHorizontal="true"
            android:background="@color/tyD1" />

        <ImageView
            android:id="@+id/mImageView_l2"
            android:layout_width="1dip"
            android:layout_height="match_parent"
            android:layout_below="@+id/mImageView_l1"
            android:layout_centerHorizontal="true"
            android:background="@color/tyD1" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/j4dp"
            android:src="@drawable/ic_option_selected" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#141414"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/mLinearLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingTop="@dimen/j5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/mTextView_time"
                    style="@style/text_style_13_white"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="2018.4.5" />

                <TextView
                    style="@style/text_style_11_gray"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableRight="@drawable/ic_detail"
                    android:gravity="center"
                    android:text="查看详细" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/j10dp"
                android:layout_marginTop="@dimen/j10dp"
                android:background="@drawable/shape_gray_bk"
                android:orientation="vertical"
                android:paddingBottom="@dimen/j10dp">

                <TextView
                    android:id="@+id/mTextView_start"
                    style="@style/text_style_13_white"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/j45dp"
                    android:layout_marginLeft="@dimen/j10dp"
                    android:layout_marginRight="@dimen/j10dp"
                    android:drawableLeft="@drawable/ic__starting"
                    android:drawablePadding="@dimen/j4dp"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="恐龙园" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/j35dp"
                    android:background="@color/tyD1" />

                <TextView
                    android:id="@+id/mTextView_end"
                    style="@style/text_style_13_white"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/j45dp"
                    android:layout_marginLeft="@dimen/j10dp"
                    android:layout_marginRight="@dimen/j10dp"
                    android:drawableLeft="@drawable/ic__destination"
                    android:drawablePadding="@dimen/j4dp"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:singleLine="true"
                    android:text="恐龙园" />

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:layout_marginLeft="@dimen/j35dp"
                    android:background="@color/tyD1" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/j68dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/mTextView_xssj"
                            style="@style/text_style_17_white"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="10"
                            android:textColor="@color/A" />

                        <TextView
                            style="@style/text_style_11_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/j8dp"
                            android:text="行驶时间(min)" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/mTextView_xslc"
                            style="@style/text_style_17_white"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="10"
                            android:textColor="@color/A" />

                        <TextView
                            style="@style/text_style_11_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/j8dp"
                            android:text="行驶里程(km)" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/mTextView_sd"
                            style="@style/text_style_17_white"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="10"
                            android:textColor="@color/A" />

                        <TextView
                            style="@style/text_style_11_gray"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/j8dp"
                            android:text="平均能耗(%)" />
                    </LinearLayout>
                </LinearLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="1dip"
                    android:background="@color/tyD1" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/j10dp"
                    android:orientation="horizontal">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:paddingRight="@dimen/j30dp">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/mTextView_csh1"
                                    style="@style/text_style_15_white"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="1" />

                                <TextView
                                    style="@style/text_style_13_white"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="次" />
                            </LinearLayout>

                            <TextView
                                style="@style/text_style_11_gray"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/j8dp"
                                android:text="急加速" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical"
                            android:paddingLeft="@dimen/j30dp">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/mTextView_csh2"
                                    style="@style/text_style_15_white"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="1" />

                                <TextView
                                    style="@style/text_style_13_white"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="次" />
                            </LinearLayout>

                            <TextView
                                style="@style/text_style_11_gray"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/j8dp"
                                android:text="急减速" />
                        </LinearLayout>


                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

