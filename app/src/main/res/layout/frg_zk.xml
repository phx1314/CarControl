<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#141414"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="@dimen/j60dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/j15dp"
        android:background="@drawable/shape_bg_left"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:paddingBottom="@dimen/j15dp">

        <LinearLayout
            android:id="@+id/mLinearLayout_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView1"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_start_nor" />

            <TextView
                android:id="@+id/mTextView_1"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="电门" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView2"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_fortification_nor" />

            <TextView
                android:id="@+id/mTextView_2"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="设防" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView3"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_defending_selected" />

            <TextView
                android:id="@+id/mTextView_3"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="解防" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/j15dp"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/mLinearLayout_wd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_control_temprature_1"
            android:gravity="bottom|center_horizontal"
            android:orientation="horizontal"
            android:paddingBottom="@dimen/j14dp">

            <TextView
                android:id="@+id/mTextView_ktwd"
                style="@style/text_style_15_white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:textSize="35sp" />

            <TextView
                style="@style/text_style_15_white"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/j10dp"
                android:text="℃"
                android:textSize="20sp" />
        </LinearLayout>

        <TextView
            style="@style/text_style_15_white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="车内温度" />

        <TextView
            style="@style/text_style_15_white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j20dp"
            android:text="车辆状态"
            android:textColor="@color/A" />

        <TextView
            android:id="@+id/mTextView_state"
            style="@style/text_style_13_white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j10dp"
            android:lineSpacingExtra="@dimen/j8dp"
            android:text="启动\n设防" />

        <TextView
            style="@style/text_style_15_white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j14dp"
            android:text="空调状态"
            android:textColor="@color/A" />

        <TextView
            android:id="@+id/mTextView_kt_state"
            style="@style/text_style_13_white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j10dp"
            android:layout_weight="1"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="@dimen/j8dp"
            android:text="制冷 25℃ 3档\n20分15s后关闭" />

        <TextView
            android:id="@+id/mImageView_cz"
            style="@style/text_style_15_white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/j38dp"
            android:layout_marginBottom="@dimen/j15dp"
            android:layout_marginLeft="@dimen/j10dp"
            android:layout_marginRight="@dimen/j10dp"
            android:layout_marginTop="@dimen/j8dp"
            android:background="@drawable/shape_login"
            android:gravity="center"
            android:padding="@dimen/j6dp"
            android:text="执行操作"
            android:textColor="@color/shouye_red" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="@dimen/j60dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/j15dp"
        android:background="@drawable/shape_bg_right"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:paddingBottom="@dimen/j15dp">

        <LinearLayout
            android:id="@+id/mLinearLayout_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView6"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:layout_marginTop="@dimen/j15dp"
                android:background="@drawable/ic_control_ac_nor" />

            <TextView
                android:id="@+id/mTextView_6"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="空调" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/mLinearLayout_7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="gone">

            <ImageView
                android:id="@+id/mImageView7"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_heating_nor" />

            <TextView
                android:id="@+id/mTextView_7"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="加热" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/mRelativeLayout_8"
            android:layout_width="@dimen/j38dp"
            android:layout_height="@dimen/j38dp"
            android:layout_marginTop="@dimen/j15dp">

            <ProgressBar
                android:id="@+id/vertical_progressbar"
                style="@style/Widget.ProgressBar.Vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:progress="0" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="@dimen/j20dp"
                android:layout_height="@dimen/j20dp"
                android:layout_centerInParent="true"
                android:background="@drawable/ic_control_timing_nor" />
        </RelativeLayout>

        <TextView
            android:id="@+id/mTextView_8"
            style="@style/text_style_13_white"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j5dp"
            android:gravity="center"
            android:text="空调定时" />

        <LinearLayout
            android:id="@+id/mLinearLayout_9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/j15dp"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mImageView9"
                android:layout_width="@dimen/j38dp"
                android:layout_height="@dimen/j38dp"
                android:background="@drawable/ic_control_mode_nor" />

            <TextView
                android:id="@+id/mTextView_9"
                style="@style/text_style_13_white"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginTop="@dimen/j5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:text="空调模式" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>

